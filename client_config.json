{
  "log": {
    "loglevel": "warning",
    "access": ""
  },
  "inbounds": [
    {
      "port": 1080,
      "protocol": "socks",
      "tag": "ws-tls-in",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      },
      "settings": {
        "auth": "noauth"
      }
    },
    {
      "port": 1081,
      "listen": "127.0.0.1",
      "protocol": "http",
      "tag": "http-in"
    }
  ],
  "outbounds": [
    {
      "protocol": "vless",
      "tag": "ws-tls-out",
      "settings": {
        "vnext": [
          {
            "address": "{{ DOMAIN: string }}",
            "port": "{{ PROXY_DOMAIN: number }}",
            "users": [
              {
                "id": "{{ UUID: string }}",
                "level": 0,
                "decrytion": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "wsSettings": {
          "path": "{{ WS_PATH: string }}"
        }
      }
    }
  ],
  "dns": {
    "servers": [
      "8.8.8.8",
      "8.8.4.4",
      "localhost"
    ]
  }
}
